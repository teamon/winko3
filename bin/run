#!/bin/sh

target="target/scala-2.9.1/classes"

for i in `ls lib`; do
    if [ ! -f "$target/$i" ]; then
        cp -v "lib/$i" "$target/$i"
    fi;
done;

CP="."
CP="$CP:$HOME/.sbt/boot/scala-2.9.1/lib/scala-library.jar"
CP="$CP:$HOME/.ivy2/cache/net.sourceforge.tuio/tuio/jars/tuio-1.4.jar"
CP="$CP:qtjambi-4.7.0.jar"

cd $target
java -XstartOnFirstThread -d32 -cp $CP winko.App
