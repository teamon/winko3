#!/bin/sh

target="target/scala-2.9.1/classes"

for i in `ls lib`; do
    if [ ! -f "$target/$i" ]; then
        cp -v "lib/$i" "$target/$i"
    fi;
done;

cd $target

java \
    -XstartOnFirstThread \
    -d32 \
    -cp $HOME/.sbt/boot/scala-2.9.1/lib/scala-library.jar:qtjambi-4.7.0.jar:. \
    winko.App
