#!/bin/sh
function die() {
    echo "$*" 1>&2
    exit 1
}

bin/fetch

if [ `uname` == "Darwin" ]; then
    export JAVA_OPTS="-XstartOnFirstThread -d32"
else
    export JAVA_OPTS="-d32"
fi;

if [ ! -x target/start ]; then
    sbt start-script
fi;

